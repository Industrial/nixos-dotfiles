auto_layouts true
copy_command "xclip -selection clipboard"
default_layout "system"
default_mode "normal"
default_shell "fish"
layout_dir "/etc/zellij/layouts"
mouse_mode true
on_force_close "quit"
scroll_buffer_size 10000
session_serialization false
theme "stylix"
ui {
  pane_frames {
    rounded_corners true
    hide_session_name false
  };
};