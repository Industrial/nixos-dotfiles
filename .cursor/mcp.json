{
  "mcpServers": {
    "git": {
      "command": "uvx",
      "args": ["mcp-server-git"],
      "env": {}
    },
    "context7": {
      "command": "bunx",
      "args": ["-y", "@upstash/context7-mcp@latest"],
      "env": {}
    },
    "chrome-devtools": {
      "command": "bash",
      "args": [
        "-c",
        "npx -y chrome-devtools-mcp@latest --isolated --executablePath /run/current-system/sw/bin/chromium"
      ]
    }
  }
}
